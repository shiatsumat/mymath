\newcommand{\leftright}[4][0]{%
	\If{\CompareInt{#1}={0}}{%
		\IfTokenEq{#2}{.}{}{#2}#4%
		\IfTokenEq{#3}{.}{}{#3}%
	}{\If{\CompareInt{#1}={1}}{%
		\bigl#2#4\bigr#3%
	}{\If{\CompareInt{#1}={2}}{%
		\Bigl#2#4\Bigr#3%
	}{\If{\CompareInt{#1}={3}}{%
		\biggl#2#4\biggr#3%
	}{\If{\CompareInt{#1}={4}}{%
		\Biggl#2#4\Biggr#3%
	}{%
		\errmessage{leftright: illegal optional argument}%
	}}}}}%
}
\newcommand{\leftmidright}[6][0]{%
	\If{\CompareInt{#1}={0}}{%
		\IfTokenEq{#2}{.}{}{#2}#5%
		\IfTokenEq{#3}{.}{}{#3}#6%
		\IfTokenEq{#4}{.}{}{#4}%
	}{\If{\CompareInt{#1}={1}}{%
		\bigl#2#5\bigm#3#6\bigr#4%
	}{\If{\CompareInt{#1}={2}}{%
		\Bigl#2#5\Bigm#3#6\Bigr#4%
	}{\If{\CompareInt{#1}={3}}{%
		\biggl#2#5\biggm#3#6\biggr#4%
	}{\If{\CompareInt{#1}={4}}{%
		\Biggl#2#5\Biggm#3#6\Biggr#4%
	}{%
		\errmessage{leftmidright: illegal optional argument}%
	}}}}}%
}

